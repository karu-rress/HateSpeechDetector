      <DependentUpon>BiasModel.mbconfig</DependentUpon>
    </None>
    <None Include="BiasModel.zip">
      <DependentUpon>BiasModel.mbconfig</DependentUpon>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup Label="HateModel">
    <None Include="HateModel.consumption.cs">
      <DependentUpon>HateModel.mbconfig</DependentUpon>
    </None>
    <None Include="HateModel.training.cs">
      <DependentUpon>HateModel.mbconfig</DependentUpon>
    </None>
    <None Include="HateModel.zip">
      <DependentUpon>HateModel.mbconfig</DependentUpon>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>