  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.ML" Version="1.6.0" />
    <PackageReference Include="Microsoft.ML.FastTree" Version="1.6.0" />
  </ItemGroup>
  <ItemGroup Label="GenderBias">
    <None Include="GenderBias.consumption.cs">
      <DependentUpon>GenderBias.mbconfig</DependentUpon>
    </None>
    <None Include="GenderBias.training.cs">
      <DependentUpon>GenderBias.mbconfig</DependentUpon>
    </None>
    <None Include="GenderBias.zip">
      <DependentUpon>GenderBias.mbconfig</DependentUpon>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup Label="BiasModel">
    <None Include="BiasModel.consumption.cs">
      <DependentUpon>BiasModel.mbconfig</DependentUpon>
    </None>
